@model pos_asp_net_mvc.Entities.Supplier
@{
    ViewBag.Title = "Supplier";
}


<h1 class="page-header">
    Supplier
    <small>Management</small>
</h1>

<ol class="breadcrumb">
    <li><a href="@Url.Action("Index", "Home")">Home</a></li>
    <li><a href="javascript:void(0);">Master</a></li>
    <li><a href="@Url.Action("Index", "Supplier")">Supplier</a></li>
    <li class="active">Detail</li>
</ol>

@if (TempData["message_success"] != null)
{
    <div class="alert alert-success alert-dismissable">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
        <p class="text-left"><i class="fa fa-check"></i>&nbsp;@Html.Raw(TempData["message_success"])</p>
    </div>
}


<div class="panel panel-default">
    <div class="panel-heading">
        <div class="clearfix">
            <div class="pull-left">
                <i class="fa fa-search"></i>&nbsp;<strong>Detail</strong>
            </div>
            <div class="pull-right">
                <a href="@Url.Action("Index", "Supplier")" class="btn btn-sm btn-primary">
                    <i class="fa fa-rotate-left"></i>&nbsp;Back
                </a>
                <a href="@Url.Action("Create", "Supplier")" class="btn btn-sm btn-success">
                    <i class="fa fa-plus"></i>&nbsp;Create New
                </a>
                <a href="@Url.Action("Edit", "Supplier", new {  id = Model.Id })" class="btn btn-sm btn-warning">
                    <i class="fa fa-edit"></i>&nbsp;Edit Data
                </a>
                <a href="@Url.Action("Delete", "Supplier", new {  id = Model.Id })" class="btn btn-sm btn-danger" id="btn-delete-data">
                    <i class="fa fa-trash"></i>&nbsp;Delete
                </a>
            </div>
        </div>
    </div>
    <div class="panel-body">
        <form class="form-horizontal">
            <div class="form-group">
                <label class="control-label col-sm-2">Name :</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@Model.Name</p>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Phone :</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@Model.Phone</p>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Email :</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@Model.Email</p>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Website :</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@Model.Website</p>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Address :</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@Model.Address</p>
                </div>
            </div>
        </form>
    </div>
    <div class="panel-footer"></div>
</div>
